{
  "name": "notes-2-self",
  "private": true,
  "description": "No-shit notes, how-to's, pointers, tips, and tricks.",
  "version": "1.0.0",
  "author": "Joseph R Miles <joe@notes2self.com>",
  "dependencies": {
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "gatsby": "^2.19.45",
    "gatsby-plugin-goatcounter": "^0.3.1",
    "gatsby-plugin-mdx": "^1.1.6",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-images": "^3.2.2",
    "gatsby-remark-numbered-footnotes": "^1.0.1",
    "gatsby-source-filesystem": "^2.2.2",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "gh-pages": "^2.2.0",
    "typescript": "^3.8.3"
  },
  "homepage": "https://github.com/joebobmiles/Notes2Self#readme",
  "keywords": [
    "gatsby",
    "empty",
    "starter"
  ],
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joebobmiles/Notes2Self.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "exit 0",
    "deploy": "npm run build && gh-pages -d public -b master",
    "deploy:travis": "npm run build && gh-pages -d public -b master -r https://$GH_TOKEN@github.com/joebobmiles/Notes2Self.git"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "/tests/.+\\.spec\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/tests/setupTests.ts"
    ]
  }
}
